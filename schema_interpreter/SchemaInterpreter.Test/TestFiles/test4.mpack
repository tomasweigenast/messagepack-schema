version:1

type User {
	uid:string 0
	first_name:string 1
	last_name:string 2
	email:string 3
	phone_number?:string 4
	email_verified:boolean 5
	phone_number_verified?:boolean 6
	payment_methods:map(PaymentMethod,boolean) 7 @(["ignore_unknown":true])
	addresses:list(Address) 8
	default_payment_method:PaymentMethod 9 = PaymentMethod.cash
}

type PaymentMethod enum {
	unknown 0
	cash 1
	debit 2
	credit 3
	bank 4
}

type Address {
	street_name:string 0
	city:string 1
	region:string 2
}